journal = {Cognitive Science},
month = {oct},
number = {4},
pages = {513--549},
title = {{On Distinguishing Epistemic from Pragmatic Action}},
url = {http://doi.wiley.com/10.1207/s15516709cog1804_1},
volume = {18},
year = {1994}
}
