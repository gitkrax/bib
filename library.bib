@article{Kirkpatrick2008,
abstract = {This paper tries to clarify the place of the handheld controller in computer game aesthetics. It starts from the premise that aesthetic form, perhaps the central category of modernist critical theory, is present in our play with computer games. The central argument is that controllers and our use of them are repressed in gameplay and that this repression facilitates a diversion of the player's energy that helps explain the compulsive nature of good games. Our sense of participation in events in game fiction is bought at the price of a loss of interest in our hands. The smooth integration of players into the rough, faltering world of gameplay is made possible by an excess of energy that passes from the unacknowledged tension in the hand into the imaginary relation we have with on-screen action.},
author = {Kirkpatrick, Graeme},
doi = {10.1177/1555412008325484},
file = {:C\:/MyTemp/Mendeley/Kirkpatrick/2008/Controller, Hand, Screen Aesthetic Form in the Computer Game.pdf:pdf},
