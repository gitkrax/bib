@book{Wardrip-Fruin2009,
address = {Cambridge Mass.},
annote = {I understand such connections as one of the things that processes can "express". At least as important, I am also concerned with the expression of ideas through processes, of which the author may or may not be aware -- and which the author may or may not intend. p.417


