@article{Graham2014,
abstract = {Empirical studies of online debate almost universally observe a “dominant” minority of posters. Informed by theories of deliberative democracy, these are typically framed negatively—yet research into their impact on debate is scant. To address this, a typology of what we call super-participation (super-posters, agenda-setters and facilitators) is developed and applied to the http://www.moneysavingexpert.com/ forum. Focusing on the first of these, we found 2,052 superposters (0.4%) contributing 47% of 25m+ posts. While superposters were quantitatively dominant, qualitative content analysis of the discursive practices of 25 superposters (n=40,044) found that most did not attempt to stop other users from posting (curbing) or attack them (flaming). In fact, in contradiction to the received wisdom, super-posters discursively performed a range of positive roles.},
author = {Graham, Todd and Wright, Scott},
doi = {10.1111/jcc4.12016},
file = {:C\:/MyTemp/Mendeley/Graham, Wright/2014/Discursive equality and everyday talk online The impact of superparticipants.pdf:pdf},
issn = {10836101},
journal = {Journal of Computer-Mediated Communication},
keywords = {Deliberation,Political talk,Public sphere,Virtual community},
number = {3},
pages = {625--642},
title = {{Discursive equality and everyday talk online: The impact of "superparticipants"}},
volume = {19},
year = {2014}
}
