@article{Kelly2015,
abstract = {This article examines the game Papers, Please to demonstrate how the aesthetic experience of gameplay resonates with the cultural logic of contemporary globalist paradigms. The author demonstrates how video games make their players under- take a synthesis of work and play via a process of psychological and physical self- modification. This interrelation between work, play, and subjectivity modification within gameplay experiences embodies the same ideological framework that gov- erns many knowledge-based economies which thrive off of user-generated content. In using the work/play/subjectivity connection to locate similarities between video games and the logic of globalist paradigms, the author presents a revised under- standing of what constitutes the political dimensions of video games and the experiences they elicit in their players. This article concludes with an analysis of how the mechanics and narrative of Papers, Please embodies the cultural mind-set of work-as-play while simultaneously challenging the discourses often applied to user- focused information technologies. Keywords},
author = {Kelly, M.},
doi = {10.1177/1555412015623897},
file = {:C\:/MyTemp/Mendeley/Kelly/2015/The Game of Politics Examining the Role of Work, Play, and Subjectivity Formation in Papers, Please.pdf:pdf},
issn = {1555-4120},
journal = {Games and Culture},
keywords = {aesthetics,game politics,globalism,labor,politics,subjectivity},
mendeley-tags = {game politics},
pages = {1--20},
title = {{The Game of Politics: Examining the Role of Work, Play, and Subjectivity Formation in Papers, Please}},
