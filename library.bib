isbn = {9781605589374},
keywords = {game design,game interpretation,procedural rhetoric},
pages = {224--231},
publisher = {ACM New York, NY, USA},
title = {{Kaboom! is a Many-Splendored Thing: An Interpretation and Design Methodology for Message-Driven Games Using Graphical Logics}},
